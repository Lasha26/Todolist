@model Todo.Domain.Entities.Do
@{
    ViewBag.Title = Model.Event;
}
<div class="detail panel">
    <div class="panel-heading">
        <h3>
            <strong>Event : @Model.Event</strong>
        </h3>
    </div>
    <div class="panel-body">
        <ul class="list-group">
            <li class="list-group-item"><h3><strong>Description :</strong></h3><h4>@Model.Description</h4></li>
            <li class="list-group-item"><h4><strong>Filling Time :</strong></h4><h5>@Model.FillingTime</h5></li>
            <li class="list-group-item"><h4><strong>Ending Time :</strong></h4><h5> @Model.EndingTime</h5></li>
            <li class="list-group-item">
                <h4><strong>Status :</strong> </h4><h5>@Model.Status</h5>
            </li>
            <li class="list-group-item"><h4><strong>Priority :</strong></h4><h5> @Model.Priority</h5></li>
        </ul>
    </div>
    <div class="back-to-list">
        @Html.ActionLink("Back to List", "List", "Do")
    </div>
</div>

